{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "chrome_url_overrides" : { "newtab": "newtab.html"},
  "permissions": ["activeTab","storage","declarativeContent"],
  "browser_action": {
    "default_popup": "newtab.html",
    "default_icon": "icon1.png"
  }
}